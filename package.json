{
  "name": "hal-app",
  "version": "0.0.1",
  "description": "A application to extract result of HAL Database",
  "main": "main.js",
  "scripts": {
    "prestart": "npm install",
    "start": "gulp && electron .",
    "test": "karma start karma.conf.js",
    "test-single-run": "karma start karma.conf.js --single-run",
    "preupdate-webdriver": "npm install",
    "update-webdriver": "webdriver-manager update",
    "preprotractor": "npm run update-webdriver",
    "build:sass": "gulp build-sass",
    "protractor": "protractor e2e-tests/protractor.conf.js",
    "package:linux": "npm run build:sass && build --publish never --linux",
    "package:windows": "npm run build:sass && build --publish never --win --x64"
  },
  "repository": "https://gitlab.com/allain.pat/mimette-app",
  "keywords": [
    "HAL",
    "CNRS",
    "Electron",
    "Archives"
  ],
  "author": "Patrick Allain <patralla@gmail.com> (https://github.com/Patouche)",
  "contributors": [
    "Am√©lie Collin"
  ],
  "license": "MIT",
  "dependencies": {
    "csv": "^1.1.0",
    "csv-parse": "^1.1.7",
    "csv-stringify": "^1.0.4",
    "stream-transform": "^0.1.1",
    "request": "^2.78.0",
    "uuid": "^3.0.0",
    "app-module-path": "^2.1.0",
    "prismjs": "~1.5.1",
    "moment": "~2.17.0",
    "electron-json-storage": "^2.0.3"
  },
  "devDependencies": {
    "angular": "~1.5.0",
    "angular-loader": "~1.5.0",
    "angular-mocks": "~1.5.0",
    "angular-route": "~1.5.0",
    "bower": "^1.7.7",
    "chai": "^3.5.0",
    "del": "2.2.2",
    "electron": "^1.4.1",
    "electron-builder": "^16.4.2",
    "electron-mocha": "^3.1.1",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "3.1.1",
    "gulp-clean": "^0.3.2",
    "gulp-clean-css": "^2.0.13",
    "gulp-concat-css": "^2.3.0",
    "gulp-istanbul": "^1.1.1",
    "gulp-load-plugins": "^1.3.0",
    "gulp-notify": "^2.2.0",
    "gulp-plumber": "^1.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^2.3.2",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-util": "^3.0.7",
    "jasmine": "^2.5.2",
    "jasmine-core": "^2.4.1",
    "jest-cli": "15.1.1",
    "karma": "1.3.0",
    "karma-chrome-launcher": "^0.2.3",
    "karma-coverage": "1.1.1",
    "karma-firefox-launcher": "^0.1.7",
    "karma-jasmine": "^1.0.2",
    "karma-junit-reporter": "^0.4.1",
    "karma-mocha-reporter": "^2.1.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "lodash": "^4.15.0",
    "phantomjs-prebuilt": "^2.1.13",
    "protractor": "^4.0.9",
    "wiredep": "4.0.0"
  },
  "build": {
    "appId": "fr.patouche.hal",
    "files": [
      "**/*",
      "node_modules/*"
    ],
    "linux": {
      "target": "tar.gz"
    },
    "win": {}
  }
}
